#!/usr/bin/env bash


# PIDQuantify-config.in for PIDQuantify
# LSZ 15 Feb 26


# this script allows any executables that uses PIDQuantify to be compiled with `PIDQuantify-config`
prefix="@CMAKE_INSTALL_PREFIX@"
libdir="@CMAKE_INSTALL_LIBDIR@"
echo "-I${prefix}/include -L${prefix}/${libdir} -lPIDQuantify -Wl,-rpath,${prefix}/${libdir}"
